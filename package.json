		"prepare": "pnpm run build",
		"prepublishOnly": "pnpm run build"
	},
	"peerDependencies": {
		"payload": ">=3.0.0"
	},
	"devDependencies": {
		"payload": "^3.65.0",
		"typescript": "^5.7.0"
	},
	"engines": {
		"node": ">=18"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/karpsw/payload-core-services.git"
	},
	"keywords": [
		"payload",
		"payloadcms",
		"plugin",
		"services",
		"cache",
		"hooks"
