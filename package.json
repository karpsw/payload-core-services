{
	"name": "payload-core-services",
	"version": "1.0.1",
	"description": "Base service layer for Payload CMS 3+: collection services, in-memory cache, cache invalidation hooks",
	"type": "module",
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"default": "./dist/index.js"
		}
	},
	"files": [
		"dist",
		"README.md"
